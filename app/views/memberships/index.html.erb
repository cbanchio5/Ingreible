
<div class="container">
  <h1 class="text-black">My Memberships</h1>
      <div class="container--favorite my-4">
        <% @memberships.each do |membership| %>
          <div class="card--favorite m-3" style="background-image: linear-gradient(rgba(0,0,0,0.2), rgba(0,0,0,0.8)), url('<%= cl_image_path membership.community.photo.key, height: 300 %>')">
            <div class="card--favorite--data">
              <h3 class="text-white favorite--title m-3"><%= membership.community.name  %></h3>
              </div>
          </div>
       <% end %>
      </div>
</div>
